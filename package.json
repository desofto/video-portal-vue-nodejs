    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pako": "^2.0.2",
    "pg": "^8.5.1",
    "redis-fast-driver": "^2.1.5",
    "uuid": "^8.3.2",
    "ws": "^7.4.2"
  },
  "devDependencies": {
