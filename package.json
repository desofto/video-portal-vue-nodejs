  "main": "index.js",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "migration": "node ./cmds/migration.js",
    "migrate": "node ./cmds/migrate.js",
    "rollback": "node ./cmds/rollback.js",
    "api:dev": "nodemon --watch . --exec node -- index.js",
    "api": "node index.js",
    "app:dev": "webpack serve --inline --progress --config webpack.config.js",
    "app:build-watch": "webpack --progress --watch",
    "app:build": "webpack --progress --mode production",
    "app:build2": "webpack --progress --optimize-minimize --mode production",
    "worker:dev": "nodemon --watch . --exec node -- worker.js",
    "worker:js": "node worker.js",
    "worker:ruby": "ruby worker.rb",
    "dev": "npm run api:dev & npm run app:dev"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
